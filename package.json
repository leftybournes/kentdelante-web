{
  "description": "Personal gitlab frontpage and portfolio",
  "main": "index.js",
  "private": false,
  "scripts": {
    "styles:watch": "node-sass -wr src/sass/application.scss public/css/app.min.css --output-style compressed",
    "styles:build": "node-sass -r src/sass -o src/css app.css --output-style expanded && postcss src/css/application.css -c postcss.config.js -o public/css/app.min.css"
  },
  "devDependencies": {
    "autoprefixer": "^9.5",
    "cssnano": "^4.1",
    "node-sass": "^4.12.0",
    "postcss": "^7.0",
    "postcss-cli": "^6.1"
  }
